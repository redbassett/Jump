#!/usr/bin/env python

import pygame, sys
from pygame.locals import *

class def Game(object):
    def __init__(self, screenSize, map):
        pygame.init()
        self.window = pygame.display.set_mode(screenSize)
        self.display.set_caption('Jump')
        self.screen = pygame.display.get_surface()
    
    def input(event):
        if event.type == QUIT:
            sys.exit(0)
    
    
